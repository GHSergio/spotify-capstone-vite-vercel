{
  "rewrites": [{ "source": "/:path*", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; style-src 'self' 'unsafe-inline' https://www.gstatic.com;"
        }
      ]
    }
  ]
}
